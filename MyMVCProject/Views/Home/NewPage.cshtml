@model List<MyMVCProject.Models.Product>

<h2>Products from Python API</h2>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Price</th>
            <th>Image</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var p in Model)
        {
            <tr>
                <td>@p.ProductId</td>
                <td>@p.Title</td>
                <td>@p.Price</td>
                <td>
                    <img src="@p.ImageUrl" width="80" />
                </td>
            </tr>
        }
    </tbody>
</table>
